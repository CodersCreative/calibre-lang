const factorial = fn (x : int) -> int => (x <= 1) ? 1 : x * factorial(x - 1);

const main = fn => {
	let n = 20;
	let result = factorial(n);
	let result = factorial(n);
	let result = factorial(n);
	let result = factorial(n);
	let result = factorial(n);
	print(result);
};

bench factorial => main();

test factorial_10 => assert(factorial(10) == 3628800, "10! did not equal 3628800 as expected");